#!/bin/bash
if ! test -e @CMAKE_BINARY_DIR@/dprobe.dat; then
  echo "not running test because Greens tables were not found"
  exit 1
fi

for prefix in a g; do
  if test -e ${prefix}@EFIT_LABEL@; then
      rm ${prefix}@EFIT_LABEL@
  fi
done

@EFIT_EXEC@ @GRID_SIZE@ << __c__MATCHING_EOF__c__
@EFIT_MODE@
@EFIT_TIME@
@EFIT_IN@

__c__MATCHING_EOF__c__


# Now check to see if files exist

for prefix in a g; do
  if ! test -e ${prefix}@EFIT_LABEL@; then
      exit 1
  fi
done

